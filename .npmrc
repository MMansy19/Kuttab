
# Disable fund messages
fund=false
# Increase network timeout for Vercel builds
network-timeout=300000
# Disable strict SSL
strict-ssl=false
# Prevents permission issues
engine-strict=false
# Ensure Prisma is correctly installed and not skipped
ignore-scripts=false
# Force Prisma to install its engines
public-hoist-pattern[]=*prisma*
public-hoist-pattern[]=*@prisma/client*
# Use Prisma CLI directly from node_modules
prefer-offline=true
node-linker=hoisted